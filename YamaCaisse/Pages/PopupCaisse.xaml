    <StackLayout x:Name="stkContent" Margin="12" Padding="24" Spacing="24" BackgroundColor="White" HorizontalOptions="Center" 
                 VerticalOptions="Center" Orientation="Vertical">
        <localView:ProduitListView x:Name="PageProduitControl" Grid.Column="1" />
        <StackLayout>
            <Button x:Name="btFermer" HorizontalOptions="End" Text="Fermer" Clicked="Closed_Clicked" />
        </StackLayout>
    </StackLayout>
</pages:PopupPage>